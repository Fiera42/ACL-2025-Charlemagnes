{
      "name": "serveur-express-mysql",
      "version": "1.0.0",
      "type": "module",
      "description": "Serveur Express simple avec MySQL",
      "main": "server.ts",
      "scripts": {
            "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\"",
            "dev:vite": "vite",
            "dev:server": "tsx watch server.ts",
            "start": "npm run build && tsx server.ts",
            "build": "vite build",
            "test": "node --import tsx --test \"**/*.test.ts\""
      },
      "dependencies": {
            "@types/better-sqlite3": "^7.6.13",
            "axios": "^1.12.2",
            "bcryptjs": "^3.0.2",
            "better-sqlite3": "^12.4.1",
            "cors": "^2.8.5",
            "dotenv": "^17.2.3",
            "express": "^5.0.0",
            "html-entities": "^2.6.0",
            "jsonwebtoken": "^9.0.2",
            "mysql2": "^3.15.0",
            "npm": "^11.6.2",
            "run": "^1.5.0",
            "tailwind-merge": "^3.3.1",
            "uuid": "^13.0.0",
            "vue": "^3.5.22",
            "vue-router": "^4.6.3"
      },
      "devDependencies": {
            "@tailwindcss/postcss": "^4.1.16",
            "@types/bcryptjs": "^2.4.6",
            "@types/cors": "^2.8.19",
            "@types/express": "^5.0.3",
            "@types/jsonwebtoken": "^9.0.10",
            "@types/node": "^24.7.0",
            "@vitejs/plugin-vue": "^6.0.1",
            "autoprefixer": "^10.4.21",
            "concurrently": "^9.2.1",
            "nodemon": "^3.1.10",
            "postcss": "^8.5.6",
            "tailwindcss": "^4.1.16",
            "ts-node": "^10.9.2",
            "tsx": "^4.20.6",
            "typescript": "^5.9.3",
            "vite": "^7.1.12"
      }
}
